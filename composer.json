{
  "name": "seothemes/genesis-starter-theme",
  "type": "wordpress-theme",
  "description": "Genesis starter theme with a modern development workflow.",
  "keywords": [
    "WordPress",
    "wordpress-theme-development",
    "genesis-framework"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Lee Anthony",
      "homepage": "https://seothemes.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/seothemes/genesis-starter-theme/issues",
    "source": "https://github.com/seothemes/genesis-starter-theme"
  },
  "repositories": [
    {
      "type": "path",
      "url": "do-not-edit/temp/seothemes/core",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": ">=5.4",
    "aristath/kirki": "^3.0",
    "richtabor/merlin-wp": "dev-master",
    "tgmpa/tgm-plugin-activation": "dev-master",
    "seothemes/core": "@dev",
    "proteusthemes/edd-theme-updater": "^0.2.1"
  },
  "autoload": {
    "psr-4": {
      "SeoThemes\\GenesisStarterTheme\\": "src/classes/"
    },
    "files": [
      "do-not-edit/vendor/richtabor/merlin-wp/class-merlin.php",
      "do-not-edit/vendor/aristath/kirki/kirki.php",
      "do-not-edit/src/functions/setup.php",
      "do-not-edit/src/functions/helpers.php",
      "do-not-edit/src/functions/shortcodes.php",
      "do-not-edit/src/structure/body.php",
      "do-not-edit/src/structure/header.php",
      "do-not-edit/src/structure/wrap.php",
      "do-not-edit/src/structure/menus.php",
      "do-not-edit/src/structure/hero.php",
      "do-not-edit/src/structure/footer.php",
      "do-not-edit/src/structure/single.php",
      "do-not-edit/src/structure/archive.php",
      "do-not-edit/src/structure/layout.php"
    ]
  },
  "config": {
    "vendor-dir": "do-not-edit/vendor",
    "preferred-install": {
      "*": "dist"
    },
    "sort-order": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "stylelint": [
      "stylelint './assets/scss/**/*.scss' --fix"
    ],
    "phpcs": [
      "phpcs"
    ]
  }
}
